## Default configuration for systemd.network
# https://github.com/systemd/systemd/blob/v235/man/systemd.network.xml

[Match]
Name=en*

[Network]
DHCP=yes
IPv6AcceptRA=yes

[DHCP]
UseDNS=yes
UseNTP=yes
UseMTU=yes
UseHostname=no
UseDomains=no
UseRoutes=yes
UseTimezone=yes
CriticalConnection=yes
RequestBroadcast=yes

[IPv6AcceptRA]
UseDNS=yes
UseDomains=no
