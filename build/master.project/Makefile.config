# The branch name that contains the "/srv" directory
serve_branch_name := service

# Configurations containing the /srv to deploy with
repository_serve := $(shell git rev-parse -q --verify $(serve_branch_name))
project_serve := $(ROOTDIR)/config/$(project).project
branch_serve := $(ROOTDIR)/srv

# List of static files that are required for deployment (does not include template)
output_config := $(boxname).config
output_keypair := $(boxname).id_rsa $(boxname).id_rsa.pub
