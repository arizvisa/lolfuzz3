# The file containing the name of the project
project_file_name := project.name

# The default message digest type to use for verifying files and provisioning
checksum_type := md5

# The default cipher to use when encrypting/decrypting stuff
cipher := aes-256-cbc

### Defaults for how to locate the service directory

# The branch name that contains the service directory
project_serve_branch := service

# The path containing a project-specific service directory
project_serve_root := config

# The path containing the contents of the service directory
project_serve_path := srv
